<body>
    <script>


        function ParImpar(num) {
            if(num%2 == 0)
            {
                return 'PAR!'
            }

            else 
            {
                return 'IMPAR!'
            }
          
        }

        let res = ParImpar(2)
        alert(res);

    </script>
