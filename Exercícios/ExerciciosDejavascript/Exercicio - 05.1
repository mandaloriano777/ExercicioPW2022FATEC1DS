<!DOCTYPE html>
<html lang="pt-br">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> Exercicio 05-1 Função nomeada </title>
</head>
    <input type="submit" id ="botao" onclick="Tabuada()" value="clique aqui para realizar a tabuada! " >
<body>

    <script>
        function Tabuada() {
            var num;

            num = parseInt(prompt("Informe o número dejseado para realizar a tabuada: "));

            for (i = 1; i <= 10; i++) {
                document.write("<br><br>" + num + " * " + i + " = " + num * i)
            }
        }
    </script>
</body>

</html>
